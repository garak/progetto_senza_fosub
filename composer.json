{
    "name": "garak/progetto-senza-fosub",
    "license": "MIT",
    "type": "project",
    "description": "Un progetto che non usa FOSUserBundle",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Dominio\\": "src/Dominio", "App\\": "src/App/" }
    },
    "autoload-dev": {
        "psr-4": { "Fixtures\\": "fixtures/", "Tests\\": "tests/" }
    },
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "doctrine/annotations": "^1.14",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16",
        "ramsey/uuid-doctrine": "^2.0",
        "sensio/framework-extra-bundle": "^6.2",
        "simple-bus/symfony-bridge": "^6.2",
        "symfony/asset": "^4.4",
        "symfony/console": "^4.4",
        "symfony/form": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/monolog-bundle": "^3.8",
        "symfony/security-bundle": "^4.4",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "beelab/test-bundle": "^5.0",
        "dama/doctrine-test-bundle": "^7.1",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/flex": "^1.9",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "^4.4",
        "symfony/web-profiler-bundle": "^4.4"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        },
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "require": "4.4.*"
        }
    }
}
