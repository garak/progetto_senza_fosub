{
    "name": "garak/progetto-senza-fosub",
    "license": "MIT",
    "type": "project",
    "description": "Un progetto che non usa FOSUserBundle",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Dominio\\": "src/Dominio", "App\\": "src/App/" }
    },
    "autoload-dev": {
        "psr-4": { "Fixtures\\": "fixtures/", "Tests\\": "tests/" }
    },
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.4",
        "simple-bus/symfony-bridge": "^5.3",
        "symfony/asset": "^4.4",
        "symfony/console": "^4.4",
        "symfony/form": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.4",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "beelab/test-bundle": "^4.3",
        "dama/doctrine-test-bundle": "^6.3",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug-bundle": "^5.0",
        "symfony/dotenv": "^4.4",
        "symfony/flex": "^1.6",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/profiler-pack": "^1.0"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "require": "4.4.*"
        }
    }
}
