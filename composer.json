{
    "name": "garak/progetto-senza-fosub",
    "license": "MIT",
    "type": "project",
    "description": "Un progetto che non usa FOSUserBundle",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Dominio\\": "src/Dominio", "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Fixtures\\": "fixtures/", "Tests\\": "tests/" }
    },
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ramsey/uuid-doctrine": "^1.2",
        "sensio/framework-extra-bundle": "^5.0",
        "simple-bus/symfony-bridge": "^5.1",
        "symfony/asset": "^3.4",
        "symfony/console": "^3.4",
        "symfony/form": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^3.4",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "^3.4",
        "symfony/twig-bundle": "^3.4",
        "symfony/validator": "^3.4",
        "symfony/yaml": "^3.4",
        "twig/extensions": "^1.4"
    },
    "require-dev": {
        "beelab/test-bundle": "^2.1",
        "dama/doctrine-test-bundle": "^5.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.8",
        "phpunit/phpunit": "^7.0",
        "symfony/browser-kit": "^3.4",
        "symfony/css-selector": "^3.4",
        "symfony/debug-bundle": "^3.4",
        "symfony/dotenv": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/phpunit-bridge": "^3.4",
        "symfony/profiler-pack": "^1.0"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "require": "3.4.*"
        }
    }
}
